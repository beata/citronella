<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>docs</target>
  </parser>
  <transformer>
    <target>docs</target>
  </transformer>
  <logging>
    <level>warn</level>
    <paths>
      <default>logs/phpdoc.log</default>
      <errors>logs/phpdoc.errors.log</errors>
    </paths>
  </logging>
  <files>
    <directory>source</directory>
    <ignore>source/vendor/*</ignore>
  </files>
</phpdoc>
